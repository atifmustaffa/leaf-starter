{
  "name": "myapp/leaf-project",
  "description": "Leaf PHP project",
  "type": "project",
  "authors": [
    {
      "name": "Atif Mustaffa",
      "homepage": "https://github.com/atifmustaffa"
    }
  ],
  "require": {
    "php": "^8.2",
    "leafs/leaf": "v4.0",
    "leafs/db": "^4.0",
    "leafs/auth": "^4.2",
    "leafs/session": "^4.0",
    "leafs/form": "^3.2",
    "leafs/csrf": "^0.5.4",
    "leafs/blade": "v4",
    "vlucas/phpdotenv": "^5.6"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": "8.2.0"
    },
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {}
}
